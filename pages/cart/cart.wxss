.all {
  position: absolute;
  width: 100%;
  top: 0;
  bottom: 100rpx;
  background: #f4f4f4;
}

.scroll {
  position: absolute;
  width: 100%;
  top: 100rpx;
  bottom: 0;
  background: white;
}

.foot {
  display: flex;
  height: 100rpx;
  position: absolute;
  bottom: 0;
  width: 100%;
  line-height: 100rpx;
}

.before {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}

.circle {
  width: 20rpx;
  height: 20rpx;
  margin-top: 40rpx;
  margin-right: 10rpx;
}

.listAll {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
  margin: 0 3%;
  width: 94%;
}

.listOne {
  text-align: right;
  padding-right: 30rpx;
}

.after {
  background: #b4282d;
  width: 200rpx;
  color: white;
  text-align: center;
}

.one {
  display: flex;
  height: 160rpx;
}

.right {
  background: #b4282d;
  color: white;
  width: 130rpx;
  height: 100%;
}

.pic {
  height: 150rpx;
  width: 150rpx;
}

.left {
  padding: 0 20rpx;
  padding-top: 60rpx;
}

.num {
  margin: 0 20rpx;
}

.center {
  flex: 1;
  margin-left: 14rpx;
  display: flex;
  align-items: center;
}


