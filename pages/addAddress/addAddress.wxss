.foot {
  position: fixed;
  width: 100%;
  height: 100rpx;
  bottom: 0;
  background: #b4282d;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.input {
  margin: 30rpx;
  padding-bottom: 10rpx;
}
.center {
  display: flex;
  justify-content: space-between;
  margin: 30rpx;
  padding-top: 30rpx;
  font-size: 30rpx
}
.icon {
  display: inline-block;
  position: relative;
  top: 5rpx;
}
.green {
  color: rgb(13, 156, 27);
}